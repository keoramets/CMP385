@page "/"
@model CMP385.Pages.loginModel
@{
}

<div class="log-form">
    <h2>Login to your account</h2>
    <form method="POST">
        <input type="text" name="username" title="username" placeholder="Username" required />
        <input type="password" name="password" title="password" placeholder="Password" required />
    </form>
    <div class="btn-container">
        <a asp-page="/admin" class="btn">Login</a>
        <a asp-page="/register-page" class="btn">Create Account</a>
    </div>
</div>

